entity File in Workspace {
    id: String,
    owner: Account,
};

namespace File {
    action get appliesTo {
        principal: Account,
        resource: File,
    };

    action download appliesTo {
        principal: Account,
        resource: File,
    };

    action update appliesTo {
        principal: Account,
        resource: File,
    };

    action delete appliesTo {
        principal: Account,
        resource: File,
    };
}